<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minHeight="450.0" prefWidth="400.0" spacing="10" styleClass="blue_panel" stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myjavafx.Admin_View_Profile_Controller">
    <Label alignment="CENTER" prefHeight="32.0" prefWidth="406.0" style="-fx-font-size: 22px; -fx-font-weight: bold;" text="View Profile">
        <VBox.margin>
            <Insets top="15.0" />
        </VBox.margin>
    </Label>
    <Label text="Username:">
        <font>
            <Font size="14.0" />
        </font>
        <VBox.margin>
            <Insets left="15.0" top="5.0" />
        </VBox.margin>
    </Label>
    <Label fx:id="usernameLabel" />
    <Label text="Date of Birth:">
        <font>
            <Font size="14.0" />
        </font>
        <VBox.margin>
            <Insets left="15.0" top="5.0" />
        </VBox.margin>
    </Label>
    <Label fx:id="dobLabel" />
    <Label text="Role:">
        <font>
            <Font size="14.0" />
        </font>
        <VBox.margin>
            <Insets left="15.0" top="5.0" />
        </VBox.margin>
    </Label>
    <Label fx:id="roleLabel" />
    <Label text="Working Hours:">
        <font>
            <Font size="14.0" />
        </font>
        <VBox.margin>
            <Insets left="15.0" top="5.0" />
        </VBox.margin>
    </Label>
    <Label fx:id="workingHoursLabel" />
    <Button fx:id="returnButton" alignment="BOTTOM_RIGHT" text="Return to Admin Home" onAction="#handleReturn">
        <font>
            <Font size="14.0" />
        </font>
        <VBox.margin>
            <Insets left="15.0" top="30.0" />
        </VBox.margin>
    </Button>
</VBox>
