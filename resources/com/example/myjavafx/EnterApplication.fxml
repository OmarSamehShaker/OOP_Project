<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" styleClass="blue_panel" stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myjavafx.EnterApplicationController">
    <children>
        <VBox alignment="CENTER" spacing="20" layoutX="50.0" layoutY="50.0">
            <Button fx:id="yesButton" text="Yes" onAction="#handleYes" prefWidth="100.0"/>
            <Button fx:id="noButton" text="No" onAction="#handleNo" prefWidth="100.0"/>
        </VBox>
    </children>
</AnchorPane>
